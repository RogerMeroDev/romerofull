package fin.coop1504.tallerjava.entidades;

public class Persona {
	protected String nombre;
	protected String identificacion;
	//protected String tipoIdentificacion;
	public Persona() {
		super();
		System.out.println("Usando constructor sin parametro");
	}
	public Persona(String nombre, String identificacion, String tipoIdentificacion) {
		super();			
		this.nombre = nombre;
		this.identificacion = identificacion;
		this.tipoIdentificacion = tipoIdentificacion;
	}

	@Override
	public String toString() {
		return "Persona [nombre=" + nombre + ", identificacion=" + identificacion + ", tipoIdentificacion="
				+ tipoIdentificacion + "]";
	}
	
}
